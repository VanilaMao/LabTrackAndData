<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit">
    <Style TargetType="xctk:IconButton" BasedOn="{StaticResource {x:Type xctk:IconButton}}">
        <Setter Property="IconLocation" Value="Left"></Setter>
        <Setter Property="MouseOverBackground" Value="White"></Setter>
        <Setter Property="MouseOverForeground" Value="Aqua"></Setter>
        <Setter Property="MousePressedForeground" Value="White"></Setter>
        <Setter Property="MousePressedBackground" Value="Blue"></Setter>
        <Setter Property="MousePressedBorderBrush" Value="Black"></Setter>
        <Setter Property="Foreground" Value="Blue"></Setter>
        <Setter Property="Background" Value="White"></Setter>
    </Style>
</ResourceDictionary>